<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% if user_signed_in? %>
  <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "dropdown-item", data: { turbolinks: false } %>
<% elsif admin_signed_in? %>
  <%= link_to "Sign Out", destroy_admin_session_path, method: :delete, class: "dropdown-item", data: { turbolinks: false } %>
<% else %>
  <%= link_to "Login", login_path, class: "btn btn-light", data: { turbolinks: false } %> OR
  <%= link_to "Register", register_path, class: "btn btn-primary", data: { turbolinks: false } %>
<% end %>